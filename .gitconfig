[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = Jack Clarke
	email = me@jackclarke.xyz
[color]
	ui = true
[credential]
	helper = store
[merge]
	tool = meld
	conflictstyle = diff3
[mergetool]
	prompt = false
[format]
	pretty = oneline
[pull]
	rebase = true
